
const toggleButton = () => {
  console.log(<%= @booking.enabled %>);
  <% if @booking.enabled == true %>
      $(".booking-<%= @booking.id %> > a").html('Enabled').removeClass('btn-warning').addClass('btn-success')
  <% else %>
      $(".booking-<%= @booking.id %> > a").html('Disabled').removeClass('btn-success').addClass('btn-warning')
  <% end %>
  $("#booking-status-<%= @booking.id %>").html("<%= @booking.enabled %>")
};

export { toggleButton };
